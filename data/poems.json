[
    {
        "title": "静夜思",
        "author": "李白",
        "content": [
            "床前明月光",
            "疑是地上霜",
            "举头望明月",
            "低头思故乡"
        ]
    },
    {
        "title": "黄鹤楼送孟浩然之广陵",
        "author": "李白",
        "content": [
            "故人西辞黄鹤楼",
            "烟花三月下扬州",
            "孤帆远影碧空尽",
            "唯见长江天际流"
        ]
    },
    {
        "title": "春晓",
        "author": "孟浩然",
        "content": [
            "春眠不觉晓",
            "处处闻啼鸟",
            "夜来风雨声",
            "花落知多少"
        ]
    }
]